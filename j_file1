pipeline {
 agent none
  stages {
    stage('checkout') {
      agent any
        steps {
          echo "checkout"
      }   
}

stage('test') {
 agent { label 'label1' }
  steps {
      echo "runnig test" 
  }
}
stage('deploy') {
  agent { label 'label2' }
  steps {
      echo "runnig deploy" 
  }
}
}
}
