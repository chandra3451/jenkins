pipeline {
 agent none
  stages {
    stage('checkout') {
      agent any
        steps {
          echo "checkout"
      }   
}

stage('test parallel') {
 parallel{
 agent { label 'label1' }
  steps {
      echo "running test" 
  }
}
stage('deploy') {
  agent { label 'label2' }
  steps {
      echo "running deploy" 
  }
}
}
}
}
